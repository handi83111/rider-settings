<application>
  <component name="CodeInsightSettings">
    <option name="completionCaseSensitive" value="2" />
    <option name="selectAutopopupSuggestionsByChars" value="true" />
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="COMPLETION_CASE_SENSITIVE" value="2" />
    <option name="SELECT_AUTOPOPUP_SUGGESTIONS_BY_CHARS" value="true" />
    <option name="AUTOCOMPLETE_ON_CODE_COMPLETION" value="false" />
    <option name="INSERT_BRACE_ON_ENTER" value="false" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="DeclarativeInlayHintsSettings">
    <option name="providerIdToEnabled">
      <map>
        <entry key="js.chain.hints" value="true" />
        <entry key="js.type.hints" value="true" />
        <entry key="ts.chain.hints" value="true" />
        <entry key="ts.enum.hints" value="true" />
        <entry key="ts.type.hints" value="true" />
      </map>
    </option>
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="STRIP_TRAILING_SPACES" value="None" />
    <option name="ENABLE_RENDERED_DOC" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_PERSISTENT" value="true" />
  </component>
  <component name="JSEditorOptions">
    <option name="ESCAPE_PASTED_TEXT" value="false" />
    <option name="CONVERT_HTML_TO_JSX" value="false" />
    <option name="ESCAPE_DOC_ON_COPY_PASTE" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;TODO)(\W|$)(.*)" />
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;BUG)(\W|$)(.*)" />
    <pattern pattern="\bNotImplementedException\b" />
  </component>
  <component name="YamlEditorOptions">
    <option name="useSmartPaste" value="false" />
  </component>
</application>